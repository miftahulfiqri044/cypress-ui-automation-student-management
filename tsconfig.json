{
    "compilerOptions": {
        "target": "es5",
        "lib": [
            "es5",
            "dom"
        ],
        "types": [
            "cypress",
            "node"
        ],
        "module": "commonjs",
        "moduleResolution": "node",
        "resolveJsonModule": true,
        "esModuleInterop": true,
        "allowJs": true,
        "noEmit": true,
        "baseUrl": ".",
        "paths": {
            "@support/*": [
                "cypress/support/*"
            ],
            "@pages/*": [
                "cypress/support/pages/*"
            ],
            "@fixtures/*": [
                "cypress/fixtures/*"
            ]
        }
    },
    "include": [
        "cypress/**/*.ts"
    ]
}